{
    "version": 2,
    "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        }
    ],
    "builds" : [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ]
}